void setup() {
  Serial.begin(115200);
  
  pinMode(relayPin01, OUTPUT);
  digitalWrite(relayPin01, HIGH); 

  if(!display.begin(SSD1306_SWITCHCAPVCC, SCREEN_ADDRESS)) {
    Serial.println(F("SSD1306 allocation failed"));
    for(;;); // Don't proceed, loop forever
  }
  delay(2000);
  
  if (! sht31.begin(0x44)) {    // Set to 0x45 for alternate i2c addr
    Serial.println("Couldn't find SHT31");
    while (1) delay(1);
  }
  
  Serial.println("Setup Done!");
  display_spu_logo();
}

void loop() {
  display_sht31();
  delay(2000);
}

void display_relay(const uint8_t * relay) {
  // Display bitmap
  display.clearDisplay();
  display.drawBitmap(0, 0, relay, 128, 64, WHITE);
  display.display();
  delay(1000);
}

void display_sht31() {
  // Get Temperature & Humidity
  float t = sht31.readTemperature();
  float h = sht31.readHumidity();
  
  if (isnan(t) || isnan(h)) {  // check if 'is not a number'
    Serial.println("Failed to read temperature");
  } 
  else { 
    String sht = "Temp : " + String(t) + " °C, Humd : "+ String(h) + " %, stateRelay01: "+ stateRelay01;
    Serial.println(sht);
  
    display.clearDisplay();
    display.setTextSize(1);
    display.setTextColor(WHITE);
    display.drawBitmap(0, 0, myTempHumd, 128, 64, WHITE);
    display.setCursor(25, 35);
    display.println(t);
    display.setCursor(95, 35);
    display.println(h);
    display.display(); 

    if(t > 30.5) {
      digitalWrite(relayPin01, LOW);   // Turn ON
      if(stateRelay01 == "OFF") {
        display_relay(myRelay1ON_2OFF);
        stateRelay01 = "ON";
      }  
    }
    else if(t < 30) {
      digitalWrite(relayPin01, HIGH);  // Turn OFF
      if(stateRelay01 == "ON") {
        display_relay(myRelay1OFF_2OFF);
        stateRelay01 = "OFF";
      }
    }
  }
}

void display_spu_logo() { 
  display.clearDisplay();
  display.drawBitmap(0, 0, SPU_LOGO, 128, 64, WHITE);
  display.display();
  delay(5000);
}
